vasmm68k_mot -m68000 -Fbin -L pi-ql.lst pi-ql.asm -o pi-ql.bin || exit
gawk 'BEGIN{D=9176;print "s/@ver@/1/;s/@maxm@/"D*7+256+32"/;s/@maxd@/"D"/g"}
/@timer@/{getline;print "s/@tioff@/"strtonum("0x"substr($1,5,8))"/"}' pi-ql.lst >z.sed
sed -f z.sed pi-ql.cbas >pi-ql.bas
gcc bin2basic.c -o bin2basic
dd if=pi-ql.bin of=pi-ql.bin1 bs=1 count=$(ls -l pi-ql.bin | awk '{print $5-20}')
bin2basic <pi-ql.bin1 >>pi-ql.bas
rm bin2basic z.sed pi-ql.bin1
